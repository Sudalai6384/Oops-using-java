import java.io.*;
import java.util.Scanner;
class Zpattern{
public static void main(String args[]){
int n;
Scanner s= new Scanner(System.in);
System.out.println("Enter the n value:");
n=s.nextInt();
for(i=0;i<=n;i++)
{
	for(j=0;j<(n+1);j++)
	{
if(i==0 || i==4){
System.out.print("@ ");}

else if(i==1&&j==4||j==5){
System.out.print("# ");}

else if(i==2&&j==2||j==3){
System.out.print("# ");}

else if(i==3&&j==0||j==1){
System.out.print("# ");}

else{
System.out.println(" ");}
}}
}}